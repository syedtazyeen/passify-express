{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/**/*.ts",
          "package.json",
          "tsconfig.json"
        ]
      }
    }
  ],
  "buildCommand": "npm run generate",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}